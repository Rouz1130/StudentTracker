
<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Templates</title>
    <link rel="stylesheet" href="https://bootswatch.com/readable/bootstrap.min.css" >
    <link rel='stylesheet' href='http://fonts.googleapis.com/css?family=Noto+Sans'  type='text/css'>
    <link rel="stylesheet" href="../Content/css/styles.css" >
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
    <script src="https://cdn.rawgit.com/vast-engineering/jquery-popup-overlay/1.7.13/jquery.popupoverlay.js"></script>
    <script src="../Content/js/easyTable.js"></script>
    <script src="Content/js/scripts.js"></script>
  </head>
  <body>
    <section class="module parallax parallax-1">
      <div class="container">
        <h1 id="brand"></h1>
      </div>
    </section>
    <section class="module content">
     <div class="container section-container">
       <table id="table">
         <thead>
           <th>First Name</th>
           <th>Last Name</th>
           <th>Email</th>
           <th>Start Date</th>
         </thead>
         <tbody>
           @foreach(var student in Model["studentList"] )
           {
           <tr>
             <td>@student.GetFName()</td>
             <td>@student.GetLName()</td>
             <td>@student.GetEmail()</td>
             <td>@student.GetStartDate()</td>
             <td><span class="glyphicon glyphicon-remove-circle"></span>
           </tr>
          }
         </tbody>
       </table>
       <br>
       <br>
       <button class="btn btn-primary" id="getSelected">Get Selected</button>
      </div>
   </section>
   <section class="module parallax parallax-1">
     <div class="container">
       <h1 id="brand"></h1>
     </div>
   </section>
   <section class="module content">
    <div class="container section-container">
      <div class="container section-container">
        <table >
          <thead>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Start Date</th>
          </thead>
          <tbody>
            <tr>
              <td>Stephen</td>
              <td>Buchner</td>
              <td>websites@jonathanbuchner.com</td>
              <td>2016-10-08</td>
              <td><span class="glyphicon glyphicon-remove-circle"></span>
            </tr>
            <tr>
              <td>Jonathan</td>
              <td>Tory</td>
              <td>websites@jonathanbuchner.com</td>
              <td>2016-10-08</td>
            </tr>
          </tbody>
        </table>
        <br>
        <br>
        <button class="btn btn-primary" id="getSelected">Get Selected</button>
    </div>
  </section>
  <section class="module parallax parallax-1">
    <div class="container">
      <h1 id="brand"></h1>
    </div>
  </section>
  <section class="module content">
   <div class="container section-container">
     <div class="city-output">
       <h1></h1>
       <p>
       </p>
     </div>
   </div>
 </section>
  </body>
</html>
 
